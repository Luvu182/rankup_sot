{
  "name": "Rankup-manager",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @Rankup-manager/backend dev",
    "dev:setup": "turbo -F @Rankup-manager/backend dev:setup"
  },
  "devDependencies": {
    "tsx": "^4.20.5",
    "turbo": "^2.5.4"
  },
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "date-fns": "^4.1.0"
  }
}
